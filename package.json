{
  "name": "react-native-socials",
  "version": "0.0.25",
  "description": "A react-native library which displays content from popular social networks",
  "main": "lib/commonjs/index.js",
  "types": "lib/typescript/src/index.d.ts",
  "license": "MIT",
  "author": "pierrecapo (https://twitter.com/PierreCaporossi)",
  "repository": {
    "type": "git",
    "url": "git://github.com/PierreCapo/react-native-socials.git"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "@types/react-native": "^0.62.1",
    "@types/react-native-video": "^5.0.1",
    "date-fns": "^2.11.1",
    "html-entities": "^1.3.1",
    "oauth-signature": "^1.5.0",
    "react-native-logs": "^3.0.3",
    "typescript": "3.8.3",
    "yarn": "^1.22.4"
  },
  "devDependencies": {
    "@react-native-community/bob": "^0.17.1",
    "tslib": "^1.11.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "bugs": {
    "url": "https://github.com/PierreCapo/react-native-socials/issues"
  },
  "homepage": "https://github.com/PierreCapo/react-native-socials#readme",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cd example && yarn start",
    "prepare": "bob build"
  },
  "keywords": [
    "react-native",
    "socials",
    "twitter",
    "instagram",
    "youtube",
    "reddit"
  ],
  "react-native": "src/index.ts",
  "module": "lib/module/index.js",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "dist"
  ],
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/",
    "lib/"
  ]
}